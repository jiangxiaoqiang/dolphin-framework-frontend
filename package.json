{
  "name": "dolphin-framework-frontend",
  "version": "1.0.0",
  "description": "dolphin front end framework",
  "main": "app.js",
  "directories": {
    "doc": "docs"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "remote-redux-devtools": "^0.5.7",
    "webpack-dev-server": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiangxiaoqiang/dolphin-framework-frontend.git"
  },
  "author": "jiangxiaoqiang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jiangxiaoqiang/dolphin-framework-frontend/issues"
  },
  "homepage": "https://github.com/jiangxiaoqiang/dolphin-framework-frontend#readme",
  "scripts": {
    "site-dev": "./node_modules/gulp/bin/gulp.js",
    "site-watch": "./node_modules/gulp/bin/gulp.js watch",
    "watch": "./node_modules/.bin/webpack-dev-server --hot --inline --content-base --port 8083 .",
    "dev": "webpack --watch --progress --colors --display-error-details --config webpack/development.config.js",
    "build": "webpack --verbose --display-error-details --config webpack/dev.config.js && ./node_modules/gulp/bin/gulp.js",
    "prod": "webpack --progress --display-error-details --config webpack/prod.config.js && ./node_modules/gulp/bin/gulp.js"
  }
}
